        </IndicatorItem>
        <IndicatorItem id="1ece5a21-d6dc-4df0-8871-b23da91441cd" condition="starts-with" preserve-case="false" negate="true">
          <Context document="imageLoadEvent" search="imageLoadEvent/filePath" type="event" />
